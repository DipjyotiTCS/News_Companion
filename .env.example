OPENAI_API_KEY=your_key_here
OPENAI_MODEL=gpt-4o-mini

# Aura requires TLS:
NEO4J_URI=neo4j+s://<your-aura-host>.databases.neo4j.io
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password

INGEST_ON_STARTUP=1
MAX_RETRIEVAL=6
# NEWS_PDF_PATH=./data/news.pdf

INGEST_MODE=deterministic

# Startup ingestion controls
FORCE_REINGEST=0
CLEAR_GRAPH_ON_REINGEST=0

# SerpAPI (for timeline + interesting facts)
SERPAPI_API_KEY=
SERPAPI_ENGINE=google_news
SERPAPI_GL=us
SERPAPI_HL=en
